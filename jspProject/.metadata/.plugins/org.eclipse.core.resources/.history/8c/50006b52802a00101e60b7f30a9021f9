<properties>
	<entry key = "getAllPost">
		SELECT ID , MEMBERID , TITLE, CONTENT
		, DATE
		FROM BOARD b 
		WHERE IS_DEL = 0;
	</entry>	
	
	<entry key ="signup">
		INSERT INTO "MEMBER"
		VALUES()
	</entry>
</properties>